    coo = checkout --ours
    cot = checkout --theirs
    cl = clone
    cp = cherry-pick
    d = diff
    dno = diff --name-only
    f = fetch
    fa = fetch --all
    fo = fetch origin
    h = help
    ha = help -a
    l = log
    m = merge
